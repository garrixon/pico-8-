 end
 if btn(⬆️) then
  h += 1
 end
 if btn(➡️) then
  w += 1
 end
 if btn(⬇️) then
  h -= 1
 end
 
 ww = w/2
 hh = h/2
end

function _draw ()
 cls()
 sspr(0,0,16,16,
 	    64-ww,64-hh,16+w,16+h)
end
